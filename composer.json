    "require": {
        "php": "^7.4 || ^8.0",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.50",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpunit/phpunit": "^9.4.2",
        "slam/php-cs-fixer-extensions": "^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "SlamFatturaElettronica\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamFatturaElettronica\\Tests\\": "tests/"
        }
    }
}
