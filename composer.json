{
    "name": "slam/php-validatore-fattura-elettronica",
    "description": "Validatore fattura elettronica",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-dom": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v3.8.0",
        "phpstan/phpstan": "^1.5.4",
        "phpstan/phpstan-phpunit": "^1.1.0",
        "phpstan/phpstan-strict-rules": "^1.1.0",
        "phpunit/phpunit": "^8.5.26 || ^9.5.20"
    },
    "autoload": {
        "psr-4": {
            "SlamFatturaElettronica\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SlamFatturaElettronica\\Tests\\": "tests/"
        }
    }
}
