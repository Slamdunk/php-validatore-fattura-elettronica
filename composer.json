        "phpstan/phpstan-strict-rules": "^1.5.2",
        "phpunit/phpunit": "^11.0.4",
        "slam/php-cs-fixer-extensions": "^3.10.0"
    },
    "autoload": {
        "psr-4": {
            "SlamFatturaElettronica\\": "lib/"
        }
