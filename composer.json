        "phpstan/phpstan-strict-rules": "^2.0.8",
        "phpunit/phpunit": "^12.5.9",
        "slam/php-cs-fixer-extensions": "^3.14.0"
    },
    "autoload": {
        "psr-4": {
            "SlamFatturaElettronica\\": "lib/"
        }
